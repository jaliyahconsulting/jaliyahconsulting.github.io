#carousel.carousel.slide(data-ride="carousel")
  ol.carousel-indicators
    li.active.carousel-slide-changer(data-target="#carousel" data-slide-to="0")
    li.carousel-slide-changer(data-target="#carousel" data-slide-to="1")
    li.carousel-slide-changer(data-target="#carousel" data-slide-to="2")
    li.carousel-slide-changer(data-target="#carousel" data-slide-to="3")

  .carousel-inner
    .active.carousel-item
      img(src="images/jaliyah-logo-fabrics-1.jpg" alt="JÃ¡liyah Logo")
    .carousel-item
      img(src="images/wendi-preach.jpg" alt="Wendi Officiating")
    .carousel-item
      img(src="images/NOPA-mass-meeting.jpg" alt="New Orleans People's Assembly")
    .carousel-item
      img(src="images/groupimg0.jpg" alt="Incite! Women of Color Against Violence")

  a.carousel-control-prev(href="#carousel" role="button" data-slide="prev")
    span.carousel-control-prev-icon(aria-hidden="true")
    span.sr-only Previous
  a.carousel-control-next(href="#carousel" role="button" data-slide="next")
    span.carousel-control-next-icon(aria-hidden="true")
    span.sr-only Next
